{
  "name": "setup-project-ts",
  "version": "1.0.0",
  "description": "setup completo para iniciar um projeto typescript",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node src/index.ts' -e ts",
    "husky:prepare": "husky install",
    "lint-staged": "lint-staged",
    "eslint": "eslint",
    "prettier": "prettier",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
